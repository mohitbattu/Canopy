pipeline{
  agent any
  stages{
    stage('build'){
      steps{
        echo "building the application.."
        echo "Application built"
      }
    }
    stage('test'){
      steps{
        echo "testing the application.."
        echo "Tested the Application"
      }
    }
    stage('deploy'){
      steps{
        echo "deploying the application.."
        echo "Deployed the Application"
      }
    }
  }
}
